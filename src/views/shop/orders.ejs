<%- include('../includes/head.ejs'); %>
    <h1>Orders</h1>
    <% if(!orders?.length){%>
        <h3>You have no orders.</h3>
    <%} else {%>
        <%orders.forEach((order) => {%>
            <h3>Order: <%=order._id%></h3>
            <ul>
                <%order.items.forEach(item => {%>
                    <li><%=item?.product?.title%> <br>QTY: <%=item.quantity%></li>
                <%})%>
            </ul>
        <%})%>
    <%}%>
<%- include('../includes/end.ejs'); %>